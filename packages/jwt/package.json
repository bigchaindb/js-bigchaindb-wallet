{
  "name": "@bigchaindb/jwt",
  "version": "0.0.18",
  "description": "JWT tool for BigChainDB wallet",
  "keywords": [
    "bigchaindb",
    "ed25519",
    "jwt"
  ],
  "license": "Apache-2.0",
  "author": "getlarge <ed@getlarge.eu>",
  "homepage": "https://github.com/bigchaindb/js-bigchaindb-wallet#readme",
  "publishConfig": {
    "cache": "~/.npm",
    "registry": "https://registry.npmjs.org/"
  },
  "publicConfig": {
    "access": "public"
  },
  "types": "dist/index",
  "main": "dist/index",
  "browser": "dist/index",
  "directories": {
    "src": "src",
    "lib": "dist",
    "test": "test"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf -rf ./dist",
    "cleanup": "rimraf -rf dist coverage",
    "compile": "tsc -p tsconfig.build.json",
    "lint": "eslint ./ --ext .js,.ts,.tsx --quiet --fix",
    "test": "jest --runInBand",
    "test:cov": "rimraf -rf ./coverage && jest --ci --runInBand --coverage",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@bigchaindb/wallet-plugins": "^0.4.3",
    "rimraf": "~3.0.2"
  },
  "dependencies": {
    "@bigchaindb/wallet-hd": "^0.4.2",
    "@bigchaindb/wallet-types": "^0.0.7",
    "jwt-nacl": "^0.0.4"
  },
  "gitHead": "d9d4d11ee7298113013dcb2895fc2160dc371837"
}
