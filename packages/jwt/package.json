{
  "name": "@bigchaindb/js-bigchaindb-wallet-jwt",
  "version": "0.0.12",
  "description": "JWT tool for BigChainDB wallet",
  "keywords": [
    "bigchaindb",
    "ed25519",
    "jwt"
  ],
  "license": "Apache-2.0",
  "author": "getlarge <ed@getlarge.eu>",
  "homepage": "https://github.com/bigchaindb/js-bigchaindb-wallets#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "publicConfig": {
    "access": "public"
  },
  "types": "dist/index",
  "main": "dist/index",
  "browser": "dist/index",
  "directories": {
    "src": "src",
    "lib": "dist",
    "test": "test"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf -rf ./dist",
    "cleanup": "rimraf -rf dist coverage",
    "compile": "tsc -p tsconfig.build.json",
    "lint": "eslint ./ --ext .js,.ts,.tsx --quiet --fix",
    "test": "jest --ci --config ../../jest.config.json ",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@bigchaindb/js-bigchaindb-wallet-plugins": "^0.2.4",
    "rimraf": "~3.0.2",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@bigchaindb/js-bigchaindb-key-derivation": "^0.2.0",
    "@bigchaindb/js-bigchaindb-wallet-types": "^0.0.3",
    "jwt-ed25519-tn": "git+https://github.com/getlarge/jsonwebtoken-ed25519-tn.git"
  },
  "gitHead": "b16cc42e43fe9130f2f5b06efd22cb660a155423"
